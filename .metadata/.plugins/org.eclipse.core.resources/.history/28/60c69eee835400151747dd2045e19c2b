package com.day2.classes;

/**
 * 
 * @author Viswanatha
 *
 */

public class AreaDemo {

	//Variables
	private final float PI = 3.1416f;
	private float radius;
	private double circleArea;
	
	public AreaDemo calculateArea(float radius){
		this.radius = radius;
		circleArea = PI * radius * radius;
		return this;
	}
	
}
